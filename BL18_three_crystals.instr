	tof_max=dd*505.555*35.6*sin((TTT)/360*PI)+delt;
	E_min=81.8049*(252.778*35.6/tof_max)*(252.778*35.6/tof_max);
	E_max=81.8049*(252.778*35.6/tof_min)*(252.778*35.6/tof_min);
*/

%}

